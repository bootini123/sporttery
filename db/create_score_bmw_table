-- Table: score_bmw

-- DROP TABLE score_bmw;

CREATE TABLE score_bmw
(
  id bigint NOT NULL,
  name text NOT NULL,
  odd_rate_bm double precision,
  odd_rate_ms double precision,
  odd_rate_bms double precision,
  
  gsd_lsd_sub double precision,
  gsd_lsd_ratio double precision,
  gsd_agbl_sub double precision,
  gsd_agbl_ratio double precision,
  gsd_albg_sub double precision,
  gsd_albg_ratio double precision,
  lsd_agbl_sub double precision,
  lsd_agbl_ratio double precision,
  lsd_albg_sub double precision,
  lsd_albg_ratio double precision,
  agbl_albg_sub double precision,
  agbl_albg_ratio double precision,
  
  gsd_lsd_bm double precision,
  gsd_lsd_ms double precision,
  gsd_lsd_bms double precision,
  gsd_agbl_bm double precision,
  gsd_agbl_ms double precision,
  gsd_agbl_bms double precision,
  gsd_albg_bm double precision,
  gsd_albg_ms double precision,
  gsd_albg_bms double precision,
  lsd_agbl_bm double precision,
  lsd_agbl_ms double precision,
  lsd_agbl_bms double precision,
  lsd_albg_bm double precision,
  lsd_albg_ms double precision,
  lsd_albg_bms double precision,
  agbl_albg_bm double precision,
  agbl_albg_ms double precision,
  agbl_albg_bms double precision,
  
  bingo text,
  result text,
  
  CONSTRAINT score_bms_pkey PRIMARY KEY (id, name)
)