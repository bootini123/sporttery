-- Table: score_bmw

-- DROP TABLE score_bmw;

CREATE TABLE score_bmw
(
  id bigint NOT NULL,
  name text NOT NULL,
  odd_rate_bm double precision,
  odd_rate_ms double precision,
  odd_rate_bms double precision,
  odd_rate_bingo text,
  
  gsd_lsd_sub double precision,
  gsd_lsd_ratio double precision,
  gsd_agbl_sub double precision,
  gsd_agbl_ratio double precision,
  gsd_albg_sub double precision,
  gsd_albg_ratio double precision,
  lsd_agbl_sub double precision,
  lsd_agbl_ratio double precision,
  lsd_albg_sub double precision,
  lsd_albg_ratio double precision,
  agbl_albg_sub double precision,
  agbl_albg_ratio double precision,
  
  gsd_lsd_quat_bm double precision,
  gsd_lsd_quat_ms double precision,
  gsd_lsd_quat_bms double precision,
  gsd_agbl_quat_bm double precision,
  gsd_agbl_quat_ms double precision,
  gsd_agbl_quat_bms double precision,
  gsd_albg_quat_bm double precision,
  gsd_albg_quat_ms double precision,
  gsd_albg_quat_bms double precision,
  lsd_agbl_quat_bm double precision,
  lsd_agbl_quat_ms double precision,
  lsd_agbl_quat_bms double precision,
  lsd_albg_quat_bm double precision,
  lsd_albg_quat_ms double precision,
  lsd_albg_quat_bms double precision,
  agbl_albg_quat_bm double precision,
  agbl_albg_quat_ms double precision,
  agbl_albg_quat_bms double precision,
  

  result text,
  
  CONSTRAINT score_bms_pkey PRIMARY KEY (id, name)
)